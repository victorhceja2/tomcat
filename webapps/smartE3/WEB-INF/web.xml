    </servlet>
    <servlet-mapping>
        <servlet-name>DBAccessService</servlet-name>
        <url-pattern>/yum.e3.Main/dbaccessservice</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>SUSAccessService</servlet-name>
