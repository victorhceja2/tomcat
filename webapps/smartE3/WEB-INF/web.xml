        <url-pattern>/SUSPaidOut</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SUSAccessService</servlet-name>
        <url-pattern>/yum.e3.Main/susaccessservice</url-pattern>
    </servlet-mapping>
    <servlet>
